{
  "name": "karma-mocha-given",
  "version": "1.0.0",
  "description": "karma adapter for mocha-given",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org>",
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/karma-mocha-given"
  },
  "scripts": {
    "test": "grunt"
  },
  "config": {
    "blanket": {
      "pattern": "///[\\w-]+\\.js$/",
