    "grunt-mocha-cov": "^0.4.0",
    "load-grunt-config": "^0.17.1",
    "time-grunt": "^1.1.1"
  },
  "dependencies": {
    "mocha": "^2.2.4",
    "mocha-given": "^0.1.3"
  }
}
